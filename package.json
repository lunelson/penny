    "node-dev": "^4.0.0",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">=10.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "verbose": true,
    "files": [
      "lib/__tests__/*.js"
    ],
    "sources": [
      "lib/*.js",
      "lib/__tests__/__fixtures__/**/*"
    ]
  }
