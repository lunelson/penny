{
  "name": "@lunelson/penny",
  "version": "2.0.0-alpha.4",
  "description": "On-the-fly live pre-processing dev server & static-site builder",
  "keywords": [
    "browser-sync",
    "no-config",
    "dev-server",
    "live-server",
    "es6",
    "pug",
    "sass",
    "webpack"
  ],
  "homepage": "https://github.com/lunelson/penny#readme",
  "bugs": {
    "url": "https://github.com/lunelson/penny/issues"
  },
  "license": "MIT",
  "author": "Lu Nelson",
  "main": "index.js",
  "bin": {
    "penny": "bin/cli.js"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunelson/penny.git"
  },
  "scripts": {
    "test": "ava",
    "eslint": "eslint .",
    "debug": "cross-env NODE_ENV=development node --inspect-brk --trace-warnings bin/cli.js serve test/penny/src",
    "start": "cross-env NODE_ENV=development node-dev --respawn --no-deps bin/cli.js serve test/penny/src",
    "serve": "cross-env NODE_ENV=development node bin/cli.js serve test/penny/src",
    "build": "cross-env NODE_ENV=production node-dev --respawn --no-deps bin/cli.js build test/penny/src test/penny/out"
  },
