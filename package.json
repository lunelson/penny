{
  "name": "@lunelson/penny",
  "version": "3.0.0-alpha.0",
  "description": "On-the-fly live pre-processing dev server & static-site builder",
  "keywords": [
    "browser-sync",
    "no-config",
    "dev-server",
    "live-server",
    "es6",
    "pug",
    "html",
    "posthtml",
    "sass",
    "css",
    "postcss",
    "webpack"
  ],
  "homepage": "https://github.com/lunelson/penny#readme",
  "bugs": {
    "url": "https://github.com/lunelson/penny/issues"
  },
  "license": "MIT",
  "author": "Lu Nelson",
  "main": "index.js",
  "bin": {
    "penny": "bin/cli.js"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunelson/penny.git"
  },
  "scripts": {
    "test": "ava",
    "eslint": "eslint .",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "reinstall": "rm -rf node_modules && npm i",
    "start": "npm run serve",
    "serve": "node-dev --respawn --no-deps bin/cli.js serve test/penny/src",
    "build": "node-dev --respawn --no-deps bin/cli.js build test/penny/src test/penny/out",
    "serve:dumb": "node bin/cli.js serve test/penny/src",
    "build:dumb": "node bin/cli.js build test/penny/src test/penny/out"
  },
  "dependencies": {
