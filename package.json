  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunelson/penny.git"
  },
  "scripts": {
    "test": "echo \"Tests are still TBD\" && exit 0",
    "debug": "cross-env NODE_ENV=development node --inspect-brk --trace-warnings bin/cli.js serve test-src",
