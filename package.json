{
  "name": "@lunelson/penny",
  "version": "2.0.0-alpha.15",
  "description": "An on-the-fly pre-processing live dev-server & static-site builder leveraging Browsersync, Webpack, Pug, Sass and PostCSS",
  "keywords": [
    "browser-sync",
    "no-config",
    "dev-server",
    "live-server",
    "pre-processor",
    "middleware",
    "postcss",
    "autoprefixer",
    "es6",
    "pug",
    "sass",
    "webpack"
  ],
  "homepage": "https://github.com/lunelson/penny#readme",
  "bugs": {
    "url": "https://github.com/lunelson/penny/issues"
  },
  "license": "MIT",
  "author": "Lu Nelson",
  "main": "index.js",
  "bin": {
    "penny": "bin/cli.js"
  },
  "directories": {
    "lib": "lib",
    "bin": "bin",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunelson/penny.git"
  },
  "scripts": {
    "test": "ava",
    "lint": "eslint .",
    "start": "npm run server",
    "serve": "cross-env NODE_ENV=development node-dev --respawn --no-deps --inspect bin/cli.js serve test/penny/src",
    "build": "cross-env NODE_ENV=production node-dev --respawn --no-deps --inspect bin/cli.js build test/penny/src test/penny/out"
  },
